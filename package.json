{
  "name": "mailstream",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "genLib": "nx generate @nrwl/js:library $NAME --publishable --importPath @mailstream/$NAME",
    "lint-changed": "nx affected --base=master --target=lint",
    "format": "nx format:write --base=master"
  },
  "private": true,
  "dependencies": {
    "axios": "^1.0.0",
    "config": "^3.3.9",
    "node-rdkafka": "^2.15.0",
    "observable-fns": "^0.6.1",
    "threads": "^1.7.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nrwl/esbuild": "15.7.1",
    "@nrwl/eslint-plugin-nx": "15.6.3",
    "@nrwl/jest": "15.7.1",
    "@nrwl/js": "15.6.3",
    "@nrwl/linter": "15.6.3",
    "@nrwl/node": "15.7.1",
    "@nrwl/react": "^15.8.1",
    "@nrwl/workspace": "15.6.3",
    "@types/config": "^3.3.0",
    "@types/jest": "28.1.1",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "esbuild": "^0.17.5",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "husky": "^8.0.3",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "lint-staged": "^13.1.1",
    "nx": "15.6.3",
    "prettier": "^2.8.4",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-organize-imports": "^3.2.2",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,css,less,ejs,json}": [
      "prettier --write"
    ]
  }
}
