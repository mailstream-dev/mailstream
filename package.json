{
  "name": "mailstream",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "genLib": "nx generate @nrwl/js:library $NAME --publishable --importPath @mailstream/$NAME",
    "genPage": "nx g @nrwl/next:page $NAME --project=fe",
    "genComponent": "nx g @nrwl/next:component $NAME --project=fe",
    "lint-changed": "nx affected --base=master --target=lint",
    "format": "nx format:write --base=master"
  },
  "private": true,
  "dependencies": {
    "axios": "^1.0.0",
    "config": "^3.3.9",
    "next": "13.1.1",
    "node-rdkafka": "^2.15.0",
    "observable-fns": "^0.6.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "threads": "^1.7.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@nrwl/cypress": "15.8.1",
    "@nrwl/esbuild": "15.7.1",
    "@nrwl/eslint-plugin-nx": "15.6.3",
    "@nrwl/jest": "15.8.1",
    "@nrwl/js": "15.8.1",
    "@nrwl/linter": "15.6.3",
    "@nrwl/next": "^15.8.1",
    "@nrwl/node": "15.7.1",
    "@nrwl/react": "15.8.1",
    "@nrwl/workspace": "15.6.3",
    "@testing-library/react": "14.0.0",
    "@types/config": "^3.3.0",
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "babel-jest": "^29.4.1",
    "cypress": "^12.2.0",
    "esbuild": "^0.17.5",
    "eslint": "~8.15.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "lint-staged": "^13.1.1",
    "nx": "15.6.3",
    "prettier": "^2.8.4",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-organize-imports": "^3.2.2",
    "react-test-renderer": "18.2.0",
    "sass": "1.55.0",
    "ts-jest": "^29.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.9.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,css,less,ejs,json}": [
      "prettier --write"
    ]
  }
}
