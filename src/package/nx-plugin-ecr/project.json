{
  "name": "nx-plugin-ecr",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "src/package/nx-plugin-ecr/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/src/package/nx-plugin-ecr",
        "main": "src/package/nx-plugin-ecr/src/index.ts",
        "tsConfig": "src/package/nx-plugin-ecr/tsconfig.lib.json",
        "assets": [
          {
            "input": "./src/package/nx-plugin-ecr/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./src/package/nx-plugin-ecr/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./src/package/nx-plugin-ecr",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./src/package/nx-plugin-ecr",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "src/package/nx-plugin-ecr/**/*.ts",
          "src/package/nx-plugin-ecr/generators.json",
          "src/package/nx-plugin-ecr/executors.json",
          "src/package/nx-plugin-ecr/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "src/package/nx-plugin-ecr/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
